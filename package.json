{
    "name": "baskom",
    "version": "0.1.23",
    "description": "Fast and fun nodejs framework with easy to use.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "clean": "rimraf lib",
        "build": "rimraf lib && tsc",
        "build:ncc": "rimraf lib && ncc build src/index.ts -o lib -m",
        "dev": "ts-node-dev --respawn src/dev.ts"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/herudi/baskom"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "author": "Herudi",
    "license": "MIT",
    "keywords": [
        "Fast nodejs web framework",
        "Baskom js for nodejs web framework",
        "Express alternative nodejs framework",
        "Simple nodejs web framework"
    ],
    "devDependencies": {
        "@types/node": "^14.14.6",
        "@vercel/ncc": "^0.26.1",
        "rimraf": "^3.0.2",
        "ts-node": "^9.0.0",
        "ts-node-dev": "^1.0.0",
        "typescript": "^4.0.5"
    },
    "dependencies": {}
}
